[upstream-ol8_automation_manager]
name = Oracle Linux $releasever Automation Manager ($basearch)
baseurl = https://yum.oracle.com/repo/OracleLinux/OL8/automation/$basearch/
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-oracle-ol8
gpgcheck = 1
enabled = 0

